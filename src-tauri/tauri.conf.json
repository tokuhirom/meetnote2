{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "meetnote2",
    "version": "0.0.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": false
    },
    "allowlist": {
      "all": false,
      "window": {
        "all": false,
        "close": true,
        "create": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "notification": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$APPDATA/**",
          "$APPDATA/*",
          "$APPDATA/",
          "$APPDATA"
        ]
      },
      "path": {
        "all": true
      },
      "dialog": {
        "confirm": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.github.tokuhirom.meetnote2",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
    ]
  }
}
